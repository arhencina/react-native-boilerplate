{
  "name": "GTOAppsBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "ios:pods": "cd ios && pod install && cd ..",
    "ios:pods:u": "cd ios && pod update && cd ..",
    "iods:pods:ru": "cd ios && pod install --repo-update && cd ..",
    "new": "hygen",
    "refresh": "rm -rf node_modules/ && yarn"
  },
  "dependencies": {
    "@react-native-community/blur": "^4.2.0",
    "@react-native-community/datetimepicker": "^6.3.1",
    "@react-native-picker/picker": "^2.4.3",
    "@reduxjs/toolkit": "^1.8.3",
    "apisauce": "^2.1.5",
    "dayjs": "^1.11.4",
    "react": "18.0.0",
    "react-native": "0.69.3",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-navigation": "^7.30.0-alpha",
    "react-native-navigation-hooks": "^6.3.0",
    "react-native-navigation-register-screens": "^1.3.0",
    "react-native-reanimated": "^2.9.1",
    "react-native-ui-lib": "^6.19.1",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-saga": "^1.1.3",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.69.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "husky": "^8.0.0",
    "hygen": "^6.2.8",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint"
  }
}
